<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="ahat.mobilemoney.NavigationDrawer"
    tools:showIn="@layout/app_bar_navigation_drawer">

    <TextView
        android:id="@+id/MsgTextView"
        android:layout_width="0dp"
        android:layout_height="22dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pay"
        app:layout_constraintRight_toLeftOf="@+id/imageButton7"
        android:layout_marginRight="0dp"
        app:layout_constraintTop_toTopOf="@+id/imageButton6"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton6"
        android:layout_marginBottom="0dp"
        app:layout_constraintVertical_bias="0.51"
        app:layout_constraintLeft_toRightOf="@+id/imageButton6"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Collect"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton5"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton5"/>

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:background="@drawable/round_border"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/ic_account_balance_wallet_black_24dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageButton6"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageButton7"
        android:layout_marginTop="38dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton6"
        app:layout_constraintHorizontal_bias="0.508"/>

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/round_border"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton6"
        app:layout_constraintLeft_toRightOf="@+id/imageButton6"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton6"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_person_black_24dp"/>

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:background="@drawable/round_border"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/qrcode"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.331"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.25"/>

</android.support.constraint.ConstraintLayout>
