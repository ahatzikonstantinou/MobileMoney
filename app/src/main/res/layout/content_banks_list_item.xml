<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/activity_banks_list_bank_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_banks_list_bank_logo"
        app:layout_constraintRight_toLeftOf="@+id/imageButton2"
        tools:text="Πειραιώς"
        app:layout_constraintLeft_toRightOf="@+id/activity_banks_list_bank_logo"
        android:layout_marginLeft="32dp"/>

    <ImageView
        android:id="@+id/activity_banks_list_bank_logo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:srcCompat="@drawable/alphabank_400x400"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintVertical_bias="0.501"
        />

    <Button
        android:id="@+id/button19"
        style="@android:style/Widget.Material.Button.Borderless"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:drawableLeft="@drawable/ic_delete_black_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_banks_list_bank_logo"
        app:layout_constraintRight_toRightOf="parent"/>

    <Button
        android:id="@+id/button20"
        style="@android:style/Widget.Material.Button.Borderless"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:drawableLeft="@drawable/ic_edit_black_24dp"
        app:layout_constraintBottom_toBottomOf="@+id/button19"
        app:layout_constraintRight_toLeftOf="@+id/button19"/>

</android.support.constraint.ConstraintLayout>
