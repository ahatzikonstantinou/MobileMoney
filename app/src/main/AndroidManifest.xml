<?xml version="1.0" encoding="utf-8"?>
<manifest package="ahat.mobilemoney"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".Register_SMS_Pin_Activity">
        </activity>
        <activity android:name=".AddBankActivity">
        </activity>
        <activity android:name=".BankActivity">
        </activity>
        <activity android:name=".EditBankActivity">
        </activity>
        <activity android:name=".SaveImportedAccountsActivity">
        </activity>
        <activity android:name=".EditAccountsActivity">
        </activity>
    </application>

</manifest>